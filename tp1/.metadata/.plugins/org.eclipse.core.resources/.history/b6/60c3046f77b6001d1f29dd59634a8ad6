package Td;

public class test {
	public static void main(String [] args) {
	try {
		EntNat e=new EntNat(4);
		System.out.println(e.getX());
		e.setX(1);
		e.decremente();
		System.out.println(e.getX());
		e.setX(5);
	}catch(ErrConst e) {
		System.err.println(e.getError()+" negative");
	}catch(ErrModif e) {
		System.err.println(e.getError()+" negative");
	}
	
	
	

	}
	
}
