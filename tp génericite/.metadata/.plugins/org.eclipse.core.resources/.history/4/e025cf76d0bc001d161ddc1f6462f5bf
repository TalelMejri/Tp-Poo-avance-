package ex2;

public class PileListe<T> implements Pile {
	private Noeud<T> sommet;
	public PileListe(){
	sommet = null;
	}
	public boolean estVide(){
	return (sommet == null);
	}
	public T dernier(){
	return sommet.info;
	}
	public void empiler(Object o){
	Noeud n = new Noeud();
	n.info = o;
	n.suivant = sommet;
	sommet = n;
	}
	public void depiler(){
}
