
public class Point<T> {
	
	private T abs;
	private T ord;
	
	public Point() {
		// TODO Auto-generated constructor stub
	}
	
	public T getAbs() {
		return abs;
	}
	public void setAbs(T abs) {
		this.abs = abs;
	}
	public T getOrd() {
		return ord;
	}
	public void setOrd(T ord) {
		this.ord = ord;
	}
	
	public String toStrig() {
		return this.ord+" "+this.abs;
	}


}
