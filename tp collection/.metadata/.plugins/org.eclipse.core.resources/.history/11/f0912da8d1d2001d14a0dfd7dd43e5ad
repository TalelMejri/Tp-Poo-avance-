package ex4;

public class Fiche {
   private String nom;
   private int numero;
   private String adresse;
   
   Fiche(String nom,int num,String adre){
	   this.nom=nom;
	   this.numero=num;
	   this.adresse=adre;
   }
   
   Fiche(String nom){
	   this.nom=nom;
	   this.adresse=null;
	   this.numero=-1;
   }
}
