package TP6;

public class Test {

	public static void main(String[] args) {
	  Entreprise e=new Entreprise("Bizo");
	  Salarié s=new Salarié(0, null, 0);
	  e.embaucherSalarie(new Salarié(15, "talel", 12));
	  e.embaucherSalarie(new Salarié(17, "mejri", 13));
	  e.embaucherSalarie(new Salarié(18, "badis", 14));
	  e.embaucherSalarie(s);
	  e.afficherSalariés();
	  e.licencierSalarie(s);
	}

}
